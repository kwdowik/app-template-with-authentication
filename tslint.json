{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "object-literal-sort-keys": false,
        "variable-name":
            [
                true,
                "ban-keywords",
                "check-format",
                "allow-leading-underscore",
                "allow-trailing-underscore",
                "allow-pascal-case",
                "allow-snake-case"
            ]
    },
    "rulesDirectory": []
}